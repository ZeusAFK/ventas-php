body
	background-color #F8F8F8
	background-image url('../images/bg.png') !important
	background-repeat repeat
	font-family 'Open Sans', sans-serif
	font-size 12px
ol, ul
	list-style none
	margin 0
	padding 0
#content
	color #333
	#cart_content
		padding 1em
		#cart_items
			padding 1em
			.product
				.category
					color #3b5998
					cursor pointer
					font-size 0.8em
					margin 0
					padding 0 0 0 1em
					text-align left
				.name
					padding 0 0 0 1em
				.name:hover
					color #3b5998
					cursor pointer
				.picture
					border 3px #eee solid
					height 70px
					width 70px
				.price_container
					table-layout fixed
					text-align right
					thead
						th
							text-align right
				.quantity
					height 25px !important
					line-height 0.9em
					margin 0
					padding 0.2em
					width 50px
				.removeIcon
					color red
					font-size 1.5em
				.removeIcon:hover
					cursor pointer
					text-shadow 0px 0px 11px rgba(255, 69, 48, 0.8)
		#map-canvas
			border 1px #8A8A8A solid
			height 400px
	#category_description
		color #333
		#category_subcategories
			opacity 1
			padding 1em
			li
				margin 0.5em
			.category_element
				color #3b5998
				cursor pointer
				.arrow_small
					color #3b5998
					font-size 0.9em
	#category_image
		background-position right bottom
		background-repeat no-repeat
		text-align center
		img
			border 3px #eee solid
			width 100%
		#change_button
			color #3b5998
			cursor pointer
		#fileupload
			display none
		#progress
			display none
			height 12px !important
	#category_products
		.product
			background #FFF
			width 100%
			.cover
				background-color #C1C4CB
				-webkit-background-size cover
				-moz-background-size cover
				-o-background-size cover
				background-size cover
				min-height 250px
				.actions
					background rgba(100, 100, 200, 0.5)
					min-height 250px
					opacity 0
					padding 1.5em
					transition visibility 0s linear 0.3s,opacity 0.3s linear
					visibility hidden
					width 100%
					.bottom
						background blue
						height 5px
						left 15px
						right 15px
						opacity 0.3
						position absolute
						top 245px
					.button_cart
						margin 1em 1.5em 1em 1.5em !important
						padding 0.2em !important
					.title
						color #FFF
						cursor pointer
						text-align center
						font-size 1.5em
						font-weight bold
					.title:hover
						text-decoration underline
					.top
						background blue
						height 5px
						left 15px
						right 15px
						opacity 0.3
						position absolute
						top 0
			.cover:hover
				.actions
					visibility visible
					opacity 1
					transition-delay 0s
			.description
				padding 0.5em 0.8em 0.1em 0.8em
				p
					margin 0
					padding 0
				.category
					color #9da0a6
					font-size 0.8em
					text-align right
				.category:hover
					color #3b5998
					cursor pointer
					font-weight bold
				.price
					color #368ae0
					font-size 1.2em
					font-weight bold
		.product:hover
			-webkit-box-shadow #bbb 0 2px 2px
			box-shadow #bbb 0 2px 2px
	.loading_header
		color #7C7699
		font-family 'Nunito' !important
		text-align center
		.title_icon
			font-size 3em !important
	#login_providers
		padding 1em
		text-align center
		.provider
			display inline-block
			margin 2em
			img
				height 35px
				margin 0
				padding 0
				width 200px
			img:hover
				background rgba(200, 200, 200, 0.5)
				border-radius 3px
				box-shadow #bbb 0 1px 1px
				cursor pointer
			.connect_button
				margin 1em
				text-align center
	#menu_left
		background #FFF
		padding 0.8em 0 0.8em 0 
		.menu_element
			padding 0.3em 0 0.3em 1em
			margin 0.8em 0 0.8em 0
			.arrow_small
				color #09f
				font-size 0.8em
			.menu_name:hover
				text-decoration underline
		li
			.sub_menu
				display none
			.category_image
				background-position right bottom
				background-repeat no-repeat
				display none
				overflow hidden
				text-align right
			.display
				display block
		.menu_element:hover
			background #09f
			color #FAFAFA
			cursor pointer
			.arrow_small
				color #FAFAFA
				font-size 0.9em
		.sub_menu
			padding 0.5em 0em 0.5em 2em
			.submenu_element
				font-size 0.9em
				padding 0.2em 0em 0.2em 1em
				.arrow_small
					color #09f
					font-size 0.8em
			.submenu_element:hover
				background #09f
				color #FAFAFA
				cursor pointer
				.arrow_small
					color #FAFAFA
					font-size 0.9em
	#nav_path
		.nav_arrow
			font-size 0.5em
			margin 0 1em 0 1em
		.nav_element
			color #3b5998
			cursor pointer
	#orders_container
		padding 1em
		.order
			opacity 0.9
			padding 1em
			.products_container
				padding 0 2em 0 2em
				.product
					color #3b5998
					cursor pointer
		.status_1
			background rgba(67, 210, 36, 0.3)
		.status_5
			background rgba(209, 221, 34, 0.3)
			opacity 0.5
		.status_5:hover
			opacity 1
		.status_3
			background rgba(255, 0, 0, 0.6)
			opacity 0.9
	#product
		font-family 'Nunito'
		#button_cart
			font-size 1.5em
			height 35px
			line-height 35px
			padding 0 !important
		#details_container
			#details
				padding 1em
		#picture_container
			max-height 500px
			overflow hidden
			text-align center
			img
				border 3px #eee solid
				width 90%
		#pictures
			.picture
				display inline-block
				height 100px
				mergin 0.5em
				padding 0.5em
				width 100px
				.image
					height 90px
					width 90px
				.image:hover
					border 2px #8A8A8A solid
					cursor pointer
		#price
			font-size 2em
			font-weight bold
		.title
			color #3b5998
	#shop_content
		#category_products
			.product
				.cover
					height 150px
					min-height 150px
					.actions
						height 150px !important
						min-height 150px
						.bottom
							top 145px
						.title
							font-size 1.2em
		.main_slider
			.slider_element
				-webkit-background-size cover
				-moz-background-size cover
				-o-background-size cover
				background-size cover
				color #FAFAFA
				height 480px
				padding 1em
				.change_button
					max-width 300px
	.box_white
		background #FFF
		border 1px solid #EAEAEA
		-webkit-border-radius 5px 5px 5px 5px
		-moz-border-radius 5px 5px 5px 5px
		border-radius 5px 5px 5px 5px
		color #333;
		margin 10px 0
		padding 10px
		width 100%
#header_container
	position fixed
	width 100%
	z-index 999
	#header
		background #3b5998
		background -webkit-linear-gradient(top, #3b5998, #163574)
		background -moz-linear-gradient(top, #3b5998, #163574)
		background -o-linear-gradient(top, #3b5998, #163574)
		background -ms-linear-gradient(top, #3b5998, #163574)
		box-shadow 0 2px 2px 0 #AAA
		color #EEE
		height 40px
		#header_content
			display none
		#logo_container
			display inline-block
			height 68px
			width 68px
			#logo
				background #fff
				-webkit-box-shadow #bbb 0 2px 2px
				box-shadow #bbb 0 2px 2px
				height 68px
				opacity 1
				width 68px
			#logo:hover
				cursor pointer
		#navigation_container
			display inline-block
			top 0px
			position absolute
			#nav_list
				.navItem
					width 93px
				.parentNav
					display inline-block
					margin 0px
					height 40px
					.nav_button
						cursor pointer
						height: 40px;
						line-height: 40px;
						text-align: center;
						text-shadow 0 1px 0 rgba(255,255,255,0.7),0 1px 0 rgba(255,255,255,0.3)
					.nav_button:hover
						background-color #628EFF
				.parentNav.On
					.nav_button
						background -webkit-linear-gradient(top, #18348D, #7696FF)
						background -moz-linear-gradient(top, #18348D, #7696FF)
						background -o-linear-gradient(top, #18348D, #7696FF)
						background -ms-linear-gradient(top, #18348D, #7696FF)
						background linear-gradient(to bottom, #18348D, #7696FF)
						-webkit-box-shadow 0 0 0 rgba(143,103,143,0),inset 0 0 4px #4B4B4B
						box-shadow 0 0 0 rgba(143,103,143,0),inset 0 0 4px #4B4B4B
					.nav_button:hover
						background-color #628EFF
		#search_container
			text-align center
			#search_button
				cursor pointer
				display inline-block
				background-color #7696FF
				border 1px solid #7696FF
				border-left none
				border-top-right-radius 3px
				border-bottom-right-radius 3px
				height 32px
				line-height 30px
				margin 5px 0 0 0
				text-align center
				width 45px
			#search_text_container
				display inline-block
				width 80%
		#user_controls_container
			text-align right
			#login_button
				display none
				height 30px
				font-size 0.9em
				line-height 30px
				margin 5px 0 0 0
				padding 0 !important
				width 150px
			#login_button:hover
				background #5CB4FF
			#controls_table
				display none
				width 100%
				#buttons_container
					#cart
						height 40px
						line-height 40px
						text-align center
						width 85px
					#cart:hover
						background-color #628EFF
						cursor pointer
					.full
						color #bfce00
						text-shadow 0px 3px 10px rgba(211, 214, 39, 1)
				#email
					font-size 0.8em
					margin 0
					padding 0 0.5em 0 0.5em
				#logout
					cursor pointer
					font-size 1.5em
					line-height 40px
					height 40px
					text-align center
				#logout:hover
					background-color #628EFF
					cursor pointer
				#name
					cursor pointer
					font-family 'Nunito'
					font-size 0.9em
					margin 0
					overflow hidden
					padding 0.5em 0.5em 0 0em
				#picture
					background #fff
					-webkit-box-shadow #bbb 0 2px 2px
					box-shadow #bbb 0 2px 2px
					height 68px
